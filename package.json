    "type": "git",
    "url": "git+https://github.com/elasticio/amqp-rpc.git"
  },
  "bugs": {
    "url": "https://github.com/elasticio/amqp-rpc/issues"
  },
  "scripts": {
    "docs": "rimraf ./docs && jsdox --output ./docs ./src",
