    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "eslint": "4.6.1",
    "eslint-config-node": "1.6.0",
    "istanbul": "0.4.5",
    "jsdox": "0.4.10",
    "mocha": "3.5.3",
    "rimraf": "2.6.2",
    "sinon": "4.1.2",
    "sinon-chai": "2.14.0"
  }
